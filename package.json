{
  "name": "twinespace",
  "version": "1.0.5",
  "description": "Twine 2 story format supporting 3D models and mixed reality projects",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "webpack && node build-format.js",
    "build:prod": "webpack --config webpack.prod.config.js && node build-format.js",
    "build:analyze": "webpack --config webpack.config.js && npx webpack-bundle-analyzer build/core.bundle.js",
    "lint": "eslint src/*.js test/*.js --fix",
    "all": "npm run lint && npm run build && npm run test",
    "docs": "serve ./docs",
    "quicktest": "extwee -c -i quicktest/index.twee -o quicktest/index.html -s docs/dist/aframe-format.js && open-cli quicktest/index.html",
    "quicktest-serve": "extwee -c -i quicktest/index.twee -o quicktest/index.html -s docs/dist/aframe-format.js && open-cli http://localhost:3000 && serve quicktest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/videlais/twine-space.git"
  },
  "keywords": [
    "twine2",
    "story-format",
    "locative",
    "mixed-reality"
  ],
  "author": "Dan Cox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/videlais/twine-space/issues"
  },
  "homepage": "https://github.com/videlais/twine-space#readme",
  "dependencies": {
    "@ar-js-org/ar.js": "^3.4.7",
    "aframe": "^1.7.1",
    "jquery": "^3.7.1",
    "markdown-it": "^14.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/css": "^0.11.0",
    "@eslint/js": "^9.35.0",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "babel-loader": "^10.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "core-js": "^3.45.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "ejs": "^3.1.10",
    "eslint": "^9.35.0",
    "eslint-plugin-jest": "^29.0.1",
    "extwee": "^2.3.3",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-puppeteer": "^11.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "open": "^10.2.0",
    "open-cli": "^8.0.0",
    "puppeteer": "^24.22.0",
    "serve": "^14.2.3",
    "shelljs": "^0.10.0",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "overrides": {
    "axios": "^1.8.2",
    "got": "^11.8.5",
    "@babel/runtime": "^7.26.10"
  }
}
